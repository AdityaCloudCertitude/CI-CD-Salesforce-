<aura:component controller="AsyncAwaitHandler" implements="force:appHostable,flexipage:availableForAllPageTypes,flexipage:availableForRecordHome,force:hasRecordId" access="global" >

    <aura:attribute name="asyncData" type="String" />
    <aura:handler name="init" value="{!this}" action="{!c.doInit}" />

    <div>
        <p>Data: {!v.asyncData}</p>
    </div>
    <button onclick="{!c.handleClick}">   click </button>
</aura:component>